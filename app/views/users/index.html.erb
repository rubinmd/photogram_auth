


<div class="row">
  <div class="col-md-8 col-md-offset-2">


    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">All Users</h3>
      </div>
      <table class="table">
        <% @users.each do |user| %>
        <tr><td><a href="users/<%= user.id %>"><%= user.username %></a></td></tr>
        <% end %>
      </table>
    </div>
  </div>
</div>

